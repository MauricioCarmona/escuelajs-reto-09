{
  "name": "platzistore",
  "version": 2,
  "builds": [{
    "src": "src/index.js",
    "use": "@now/node"
}],
"routes": [{
    "src": "/(.*)",
    "dest": "/src/index.js"
}],
  "env": {
    "DB_USER": "@reto09-db-user",
    "DB_PASSWORD": "@reto09-password",
    "DB_HOST": "@reto09-db-host",
    "DB_NAME": "@reto09-db-name"
  }
}
